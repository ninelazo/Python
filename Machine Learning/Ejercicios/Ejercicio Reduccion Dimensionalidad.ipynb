   "source": [
    "# Utilizamos el Standard Scaler para escalar los registros entre valores de -1 y 1\n",
    "sc = StandardScaler()\n",
    "x_train = sc.fit_transform(x_train)\n",
    "x_val = sc.transform(x_val)\n",
    "x_test = sc.transform(x_test)"
   ]
